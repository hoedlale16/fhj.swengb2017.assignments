<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="540.0" prefWidth="752.0" styleClass="bg_game" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.fhj.swengb.apps.battleship.jfx.BattleShipHighscoreFxController">
    <right>


    </right>
    <top>
        <Label fx:id="lbHeader" contentDisplay="CENTER" styleClass="lb_header" text="Battleship - Highscore" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" BorderPane.alignment="BOTTOM_CENTER">
            <font>
                <Font size="27.0" />
            </font>
        </Label>
    </top>
   <bottom>
      <BorderPane prefHeight="51.0" prefWidth="752.0" BorderPane.alignment="CENTER">
         <right>
            <Button id="button" fx:id="btReturnToMain" onAction="#returnToMain" prefHeight="27.0" prefWidth="142.0" styleClass="buttons" text="Return to Main" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </bottom>
   <center>
      <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <BorderPane>
               <center>
                  <TableView fx:id="tbHighscore" BorderPane.alignment="CENTER">
                    <columns>
                      <TableColumn prefWidth="75.0" text="C1" />
                      <TableColumn prefWidth="75.0" text="C2" />
                    </columns>
                  </TableView>
               </center>
            </BorderPane>
         </content>
      </ScrollPane>
   </center>
</BorderPane>
